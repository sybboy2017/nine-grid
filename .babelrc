{
	"presets": [
		[
			"es2015",
			{
				"modules": false
			}
		],
		"stage-0"
	],
	"env": {
		"test": {
			"plugins": [
				"transform-es2015-modules-commonjs"
			]
		},
		"development": {
			"presets": []
		}
	},
	"sourceMaps": true,
	"retainLines": true,
	"plugins": [
		"transform-runtime",
		"transform-decorators-legacy"
	]
}
